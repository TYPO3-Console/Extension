{
    "name": "typo3-console/sql-command",
    "description": "TYPO3 Console command that executes SQL",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Helmut Hummel",
            "email": "info@helhum.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Typo3Console\\SQLCommand\\": "src/"
        }
    },
    "extra": {
        "typo3/cms": {
            "Package": {
                "serviceProvider": "Typo3Console\\SQLCommand\\ServiceProvider",
                "protected": true,
                "partOfMinimalUsableSystem": true
            },
            "extension-key": "typo3_console_sql"
        }
    }
}
